{
  "name": "desktime",
  "version": "1.0.0",
  "description": "Tracking app",
  "main": "main.js",
  "scripts": {
    "start": "npx electron .",
    "dist": "electron-builder",
    "rebuild": "electron-rebuild"
  },
  "build": {
    "appId": "com.vijay.desktime",
    "productName": "DeskTime",
    "files": [
      "**/*",
      "!node_modules/.cache"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    },
    "asarUnpack": [
      "**/node_modules/screenshot-desktop/**"
    ]
  },
  "author": "Vijay Ram",
  "license": "ISC",
  "dependencies": {
    "@nut-tree-fork/nut-js": "^4.2.6",
    "active-win": "^8.2.1",
    "auto-launch": "^5.0.6",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "electron-log": "^5.4.1",
    "electron-store": "^8.1.0",
    "express": "^5.1.0",
    "form-data": "^4.0.3",
    "moment-timezone": "^0.6.0",
    "node-addon-api": "^8.4.0",
    "screenshot-desktop": "^1.15.1"
  },
  "devDependencies": {
    "electron": "^36.4.0",
    "electron-builder": "^26.0.12"
  }
}
